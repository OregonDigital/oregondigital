<div id="ingest-form-container">
  <%= ingest_form_for(@form) do |f| %>
    <%= form_errors(f) %>
    <%= render "file_upload_field", :asset => @form.asset, :upload => @form.upload, :f => f %>
    <%= render "internal_group_fields", :form => @form.form, :f => f %>
    <%= render "raw_statements", :form => @form %>
    <%= form_submit(f) %>
  <% end %>
</div>

<script>
  var controlledVocabMap = <%= raw(@controlled_vocab_map.to_json) %>;
</script>
